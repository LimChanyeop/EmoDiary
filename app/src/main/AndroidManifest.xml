<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.emodiary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EmoDiary">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DescribeEmotion"
            android:label="DescribeEmotion">
        </activity>
        <activity
            android:name=".RecordList"
            android:label="RecordList">
        </activity>
        <activity
            android:name=".SelectEmoPath"
            android:label="SelectEmoPath">
        </activity>
        <activity
            android:name=".SelectEmoPath_Specific"
            android:label="SelectEmoPath_Specific">
        </activity>
    </application>

</manifest>